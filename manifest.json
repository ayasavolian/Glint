{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
     "default_popup": "popup.html",
      "default_icon": "images/favicon.png",
      "default_title": "Glint Demo"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
       "run_at": "document_start",
      "matches": ["*://app.glintinc.com/thrive@demo2*" ]
   } ],
   "description": "This is a plugin to manipulate the front-end for Glint demos",
   "icons": {
      "128": "images/favicon.png",
      "16": "images/favicon.png",
      "48": "images/favicon.png"
   },
   "manifest_version": 2,
   "name": "Glint Demo",
   "permissions": ["notifications", "<all_urls>", "management", "storage", "webNavigation", "tabs", "cookies" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3.2"
}